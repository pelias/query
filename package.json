{
  "name": "pelias-query",
  "version": "0.0.0-semantic-release",
  "description": "An Elasticsearch query builder for Pelias",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "index.js",
  "scripts": {
    "lint": "jshint .",
    "units": "node test/run.js | tap-spec",
